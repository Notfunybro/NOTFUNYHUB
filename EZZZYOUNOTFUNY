local gameId = 335760407 

if game.PlaceId == gameId then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Notfunybro/Tpstrialezzz/main/77_LR3FBD.lua"))()
else
    warn("Este script solo es compatible con el juego TPS Street Soccer.")
end

